{
  "name": "mattermost-video-plugin-webapp",
  "version": "1.0.0",
  "description": "Веб-часть плагина для просмотра видео в Mattermost",
  "main": "src/index.tsx",
  "scripts": {
    "build": "webpack --mode=production",
    "build:watch": "webpack --mode=development --watch",
    "debug": "webpack --mode=development",
    "lint": "eslint --ignore-pattern node_modules --ignore-pattern dist --ext .js,.jsx,.ts,.tsx --quiet .",
    "fix": "eslint --ignore-pattern node_modules --ignore-pattern dist --ext .js,.jsx,.ts,.tsx --quiet . --fix",
    "test": "jest --forceExit --detectOpenHandles --verbose",
    "test:watch": "jest --watch",
    "test-ci": "jest --forceExit --detectOpenHandles --maxWorkers=2"
  },
  "author": "Mattermost Video Plugin Team",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.0",
    "@babel/plugin-proposal-optional-chaining": "^7.17.12",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.18.0",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@babel/runtime": "^7.18.0",
    "@types/jest": "^28.1.0",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^28.1.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-typescript-to-proptypes": "^2.0.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.16.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "file-loader": "^6.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.0",
    "jest-canvas-mock": "^2.4.0",
    "jest-junit": "^14.0.0",
    "sass": "^1.52.0",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.3.1",
    "typescript": "^4.7.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "redux": "^4.2.0"
  },
  "jest": {
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/non_npm_dependencies/"
    ],
    "clearMocks": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "coverageReporters": [
      "lcov",
      "text-summary"
    ],
    "moduleNameMapping": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy",
      "^.+\\.(css|less|scss)$": "identity-obj-proxy",
      "^.*\\.scss$": "identity-obj-proxy"
    },
    "moduleDirectories": [
      "",
      "node_modules",
      "non_npm_dependencies"
    ],
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ],
    "testEnvironment": "jsdom"
  }
} 